{
    "name": "rakibdevs/laravel-query-extra",
    "description": "Run complex SQL queries from API requests such as update different conditional records in a single query",
    "keywords": ["laravel","query","query builder","bulk update"],
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Rakibul Islam",
            "email": "rakib1708@gmail.com",
            "website": "https:://rakibul.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
     "require-dev": {
        "orchestra/testbench": "^4.9|^5.8|^6.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RakibDevs\\QueryExtra\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RakibDevs\\QueryExtra\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RakibDevs\\QueryExtra\\QueryExtraServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
